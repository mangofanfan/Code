{
  "compilerOptions": {
    "target": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "lib": ["ESNext", "DOM"],
    "types": ["@vue/runtime-core"],
    "module": "ESNext",
    "moduleResolution": "bundler",
  },
  "include": ["src"]
}
